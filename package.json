{
	"name": "helloWorld",
	"displayName": "",
	"description": "LEAVE BLANK",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.69.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:helloWorld.helloWorld",
		"onView:package-subscriptions"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": {
			"command": "helloWorld.helloWorld",
			"title": "Hello World"
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "package-explorer",
					"title": "Package Explorer",
					"icon": "$(cloud)"
				}
			]
		},
		"views": {
			"package-explorer": [
				{
					"id": "package-subscriptions",
					"name": "Subscriptions"
				},
				{
					"id": "package-outline",
					"name": "Outline"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "16.x",
		"@types/vscode": "^1.69.0",
		"@typescript-eslint/eslint-plugin": "^5.30.0",
		"@typescript-eslint/parser": "^5.30.0",
		"@vscode/test-electron": "^2.1.5",
		"eslint": "^8.18.0",
		"glob": "^8.0.3",
		"mocha": "^10.0.0",
		"typescript": "^4.7.4"
	},
	"dependencies": {
		"axios": "^0.27.2",
		"jsonc-parser": "^3.1.0"
	}
}
